#include <iostream>
#include <algorithm>

using namespace std;

int MCD_B(int a , int b){
  for(int i = min(a , b); i > 0; i--){
    if((a % i) == 0 && (b % i) == 0)
      return i;
  }
}


int main(){
  int a , b;
  cin >> a >> b;
  cout << MCD_B(a , b) << endl;

 
  return 0;
}
